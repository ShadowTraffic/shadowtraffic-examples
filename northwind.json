                    "n": 5
                },
                "company_name": {
                    "_gen": "string",
                    "expr": "#{Company.name}"
                },
                "contact_name": {
                    "_gen": "string",
                    "expr": "#{Name.fullName}"
                },
                "contact_title": {
                    "_gen": "oneOf",
                    "choices": [
                        "Owner",
                        "Sales Representative",
                        "Marketing Manager",
                        "Sales Manager",
                        "Accounting Manager",
                        "Sales Associate",
                        "Order Administrator"
                    ]
                },
                "address": {
                    "_gen": "string",
                    "expr": "#{Address.streetAddress}"
                },
                "city": {
                    "_gen": "string",
                    "expr": "#{Address.city}"
                },
                "region": {
                    "_gen": "string",
                    "expr": "#{Address.state}",
