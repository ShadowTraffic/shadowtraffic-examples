                "address": {
                    "_gen": "string",
                    "expr": "#{Address.streetAddress}"
                },
                "city": {
                    "_gen": "string",
                    "expr": "#{Address.city}"
                },
                "region": {
                    "_gen": "string",
                    "expr": "#{Address.state}",
