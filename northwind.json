            "localConfigs": {
                "maxEvents": {
                    "_gen": "uniformDistribution",
                    "bounds": [1, 7]
                }
            },
            "vars": {
                "product": {
                    "_gen": "lookup",
                    "table": "products",
                    "path": [
                        "row"
                    ]
                }
            },
            "row": {
                "order_id": {
                    "_gen": "var",
                    "var": "forkKey",
                    "path": [
                        "row",
                        "order_id"
                    ]
                },
                "product_id": {
                    "_gen": "var",
                    "var": "product",
                    "path": [
                        "product_id"
                    ]
                },
                "unit_price": {
                    "_gen": "var",
                    "var": "product",
                    "path": [
                        "unit_price"
                    ]
                },
                "quantity": {
                    "_gen": "uniformDistribution",
                    "bounds": [
                        1,
                        130
                    ],
                    "decimals": 0
                },
                "discount": {
                    "_gen": "weightedOneOf",
