{
    "generators": [
        {
            "table": "orders",
            "row": {
                "orderId": {
                    "_gen": "uuid"
                },
                "orderDate": {
                    "_gen": "dateToday"
                }
            },
            "localConfigs": {
                "events": {
                    "exactly": 15
                }
            }
        }
    ],
    "connections": {
        "pg": {
            "kind": "postgres",
            "connectionConfigs": {
                "host": "localhost",
                "port": 5432,
                "db": "mydb"
            }
        }
    }
}
